{
  "name": "regina_online_tests",
  "version": "2.0.0",
  "description": "Comprehensive E2E Test Framework for https://reginaonline.de/",
  "main": "index.js",
  "scripts": {
    "test": "node --require tsconfig-paths/register node_modules/@playwright/test/cli.js test",
    "clean:reports": "rimraf test-results/* allure-results/* || true",
    "pretest": "npm run clean:reports",
    "test:smoke": "cross-env TEST_TYPE=smoke PW_PROJECT=chromium node --require tsconfig-paths/register node_modules/@playwright/test/cli.js test --grep '@smoke'",
    "test:smoke:prod": "cross-env TARGET=prod TEST_TYPE=smoke playwright test --grep '@smoke' --project=chromium",
    "test:regression": "cross-env TEST_TYPE=regression playwright test --grep '@regression|@smoke'",
    "test:smoke:desktop": "cross-env TEST_TYPE=smoke-desktop playwright test --grep '(@smoke.*@desktop|@desktop.*@smoke)'",
    "test:smoke:mobile": "cross-env TEST_TYPE=smoke-mobile playwright test --grep '(@smoke.*@mobile|@mobile.*@smoke)'",
    "test:regression:desktop": "cross-env TEST_TYPE=regression-desktop playwright test --grep '(@regression.*@desktop|@desktop.*@regression|@smoke.*@desktop|@desktop.*@smoke)'",
    "test:regression:mobile": "cross-env TEST_TYPE=regression-mobile playwright test --grep '(@regression.*@mobile|@mobile.*@regression|@smoke.*@mobile|@mobile.*@smoke)'",
    "test:ui": "playwright test src/tests/ui",
    "test:api": "playwright test --project=api",
    "test:chrome": "node --require tsconfig-paths/register node_modules/@playwright/test/cli.js test --project=chromium",
    "test:safari": "node --require tsconfig-paths/register node_modules/@playwright/test/cli.js test --project=webkit",
    "test:mobile": "playwright test --project=iphone --project=ipad-mini",
    "test:local": "cross-env TARGET=local playwright test",
    "test:prod": "cross-env TARGET=prod playwright test",
    "test:smoke:local": "cross-env TARGET=local TEST_TYPE=smoke playwright test --grep '@smoke' --project=chromium",
    "test:regression:local": "cross-env TARGET=local TEST_TYPE=regression playwright test --grep '@regression|@smoke'",
    "test:regression:prod": "cross-env TARGET=prod TEST_TYPE=regression playwright test --grep '@regression|@smoke'",
    "test:headed": "cross-env HEADED=true playwright test",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "report:allure": "allure serve test-results/allure-results",
    "report:generate": "allure generate test-results/allure-results --clean -o test-results/allure-report",
    "show-trace": "npx playwright show-trace",
    "codegen": "playwright codegen $TEST_BASE_URL || echo 'Set TEST_BASE_URL'",
    "codegen:local": "TARGET=local playwright codegen http://localhost:3000",
    "codegen:prod": "TARGET=prod playwright codegen https://reginaonline.de",
    "install:browsers": "npx playwright install --with-deps",
    "docker:build": "docker build -t regina-e2e-tests .",
    "docker:run": "docker run --rm -v $(pwd)/test-results:/app/test-results regina-e2e-tests",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint:fix-nullish": "eslint . --ext .ts,.js --fix --rule '@typescript-eslint/prefer-nullish-coalescing: error'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format-and-lint": "npm run format && npm run lint:fix",
    "check-all": "npm run format && npm run lint:fix && npm run type-check",
    "type-check": "tsc --noEmit",
    "deadcode:check": "ts-unused-exports tsconfig.json",
    "deadcode:check:verbose": "ts-unused-exports tsconfig.json --showLineNumber",
    "deadcode:check:framework": "ts-unused-exports tsconfig.json --excludePathsFromReport=src/pom,src/utils,src/types,src/config,src/data,src/fixtures,src/mocks",
    "deadcode:check:tests-only": "ts-unused-exports tsconfig.json --searchNamespaces --findCompletelyUnusedFiles src/tests",
    "deadcode:json": "ts-unused-exports tsconfig.json --json",
    "deps:check": "depcheck",
    "deps:check:verbose": "depcheck --detailed",
    "deps:check:json": "depcheck --json",
    "prepare": "husky install",
    "pre-commit": "npm run lint && npm run type-check"
  },
  "keywords": [
    "playwright",
    "e2e-testing",
    "automation",
    "testing",
    "typescript",
    "ci-cd"
  ],
  "author": "Chepkunova Regina",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "1.55.0",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "allure-js-commons": "^3.2.2",
    "allure-playwright": "^3.2.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "module-alias": "^2.2.3",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.4.2",
    "rimraf": "^6.0.1",
    "ts-unused-exports": "^10.1.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "uuid": "^10.0.0",
    "winston": "^3.17.0"
  },
  "_moduleAliases": {
    "@fixtures": "src/fixtures",
    "@utils": "src/utils",
    "@pom": "src/pom",
    "@tests": "src/tests",
    "@test-data": "src/data"
  }
}
